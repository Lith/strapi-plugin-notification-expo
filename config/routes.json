    {
      "method": "GET",
      "path": "/exponotifications/:id",
      "handler": "exponotification.findOne",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "PUT",
      "path": "/exponotifications/:id",
      "handler": "exponotification.update",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "POST",
      "path": "/exponotifications",
      "handler": "exponotification.create",
      "config": {
        "policies": ["isAdmin"]
      }
    },
    {
      "method": "DELETE",
      "path": "/exponotifications/:id",
      "handler": "exponotification.delete",
      "config": {
        "policies": ["isAdmin"]
      }
    }
